# Database
POSTGRES_USER=interview
POSTGRES_PASSWORD=change_this_strong_password
POSTGRES_DB=interview_bot
DATABASE_URL=postgresql://interview:change_this_strong_password@localhost:5432/interview_bot

# Auth
APP_PASSWORD=your-password-here
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRES_IN=7d

# AI Proxy
AI_PROXY_URL=http://localhost:3456
AI_MODEL=claude-sonnet-4

# App
NEXT_PUBLIC_APP_NAME=Interview Bot
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Stripe
STRIPE_SECRET_KEY=sk_test_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PRICE_PRO_MONTHLY=price_...
STRIPE_PRICE_PRO_YEARLY=price_...

# Cron (optional)
CRON_SECRET=your-cron-secret

# Redis (optional)
REDIS_URL=redis://localhost:6379

# Email (optional)
RESEND_API_KEY=re_...
EMAIL_FROM=InterviewBot <noreply@interviewbot.com>

# Admin & Security (optional)
ADMIN_ALLOWED_IPS=127.0.0.1,::1
TRUSTED_PROXY_IPS=127.0.0.1,::1

# Logging (optional)
DISABLE_TOKEN_LOGGING=false

# Database Seeding (optional)
INITIAL_USER_PASSWORD=your-initial-password
